{
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2017
  },

  "env": {
    "node": true,
    "browser": true,
    "es6": true,
    "commonjs": true
  },

  "globals": {
    "RCS": false,
    "PIXI": false,
    "is": false,
    "math": false,
    "_": false,
    "URI": false,
    "Buffer": false,
    "localStorage": false,
    "setTimeout": false,
    "clearTimeout": false,
    "setInterval": false,
    "clearInterval": false,
    "Mixin": false,
    "mix": false
  },

  "extends": [
  ],

  "plugins": [
  ],

  "rules": {
    "arrow-parens": [
      1,
      "as-needed"
    ],
    "arrow-spacing": [
      1,
      {
        "before": true,
        "after": true
      }
    ],
    "no-class-assign": 2,
    "no-const-assign": 2,
    "prefer-const": 0,
    "no-dupe-class-members": 2,
    "object-shorthand": 0,
    "prefer-arrow-callback": 1,
    "comma-dangle": 0,
    "no-cond-assign": 0,
    "no-constant-condition": 0,
    "no-dupe-args": 1,
    "no-dupe-keys": 0,
    "no-duplicate-case": 1,
    "no-empty-character-class": 1,
    "no-ex-assign": 1,
    "no-extra-boolean-cast": 1,
    "no-extra-parens": [
      1,
      "all"
    ],
    "handle-callback-err": 0,
    "no-extra-semi": 1,
    "no-func-assign": 1,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 1,
    "no-sparse-arrays": 1,
    "no-unreachable": 2,
    "no-unmodified-loop-condition": 0,
    "no-unexpected-multiline": 2,
    "block-scoped-var": 2,
    "consistent-return": 2,
    "curly": 1,
    "dot-location": [
      1,
      "property"
    ],
    "eqeqeq": [
      1,
      "smart"
    ],
    "no-eq-null": 1,
    "no-eval": 2,
    "no-extra-bind": 1,
    "no-fallthrough": 1,
    "no-floating-decimal": 1,
    "no-implicit-coercion": 1,
    "no-implied-eval": 2,
    "no-invalid-this": 0,
    "no-labels": 2,
    "no-lone-blocks": 0,
    "no-loop-func": 0,
    "no-multi-spaces": 1,
    "no-native-reassign": 1,
    "no-new-func": 0,
    "no-new": 1,
    "no-octal": 1,
    "no-proto": 1,
    "no-redeclare": 1,
    "no-return-assign": 1,
    "no-script-url": 1,
    "no-self-compare": 1,
    "no-sequences": 1,
    "no-unused-expressions": 1,
    "no-useless-call": 1,
    "no-void": 1,
    "no-with": 1,
    "wrap-iife": 0,
    "yoda": [
      1,
      "never",
      {
        "exceptRange": false,
        "onlyEquality": false
      }
    ],
    "no-shadow-restricted-names": 1,
    "no-undef-init": 2,
    "no-undef": 2,
    "no-unused-vars": 1,
    "no-use-before-define": 1,
    "no-multi-str": 0,
    "array-bracket-spacing": [
      1,
      "never",
      {
        "singleValue": false,
        "objectsInArrays": false,
        "arraysInArrays": true
      }
    ],
    "block-spacing": [
      1,
      "always"
    ],
    "brace-style": [
      1,
      "stroustrup",
      {
        "allowSingleLine": true
      }
    ],
    "camelcase": [
      0,
      {
        "properties": "always"
      }
    ],
    "comma-spacing": [
      1,
      {
        "before": false,
        "after": true
      }
    ],
    "comma-style": [
      1,
      "last"
    ],
    "computed-property-spacing": [
      1,
      "never"
    ],
    "consistent-this": [
      1,
      "that"
    ],
    "eol-last": 2,
    "func-style": [
      2,
      "expression",
      {
        "allowArrowFunctions": true
      }
    ],
    "indent": [
      1,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "key-spacing": [
      1,
      {
        "afterColon": true,
        "beforeColon": false
      }
    ],
    "lines-around-comment": [
      1,
      {
        "beforeBlockComment": true,
        "beforeLineComment": false,
        "allowBlockStart": true,
        "allowBlockEnd": true
      }
    ],
    "new-parens": 2,
    "no-lonely-if": 1,
    "no-mixed-spaces-and-tabs": 1,
    "no-multiple-empty-lines": [
      1,
      {
        "max": 2
      }
    ],
    "no-spaced-func": 0,
    "no-trailing-spaces": 1,
    "no-throw-literal": 0,
    "object-curly-spacing": [
      1,
      "always",
      {
        "objectsInObjects": true,
        "arraysInObjects": true
      }
    ],
    "one-var": [
      1,
      "never"
    ],
    "operator-assignment": [
      1,
      "always"
    ],
    "operator-linebreak": [
      1,
      "after"
    ],
    "quote-props": [
      2,
      "as-needed"
    ],
    "quotes": [
      1,
      "single",
      "avoid-escape"
    ],
    "semi-spacing": [
      1,
      {
        "before": false,
        "after": true
      }
    ],
    "semi": [
      1,
      "never"
    ],
    "require-yield": 1,
    "keyword-spacing": 2,
    "space-before-blocks": [
      1,
      "always"
    ],
    "space-before-function-paren": [
      1,
      "always"
    ],
    "space-in-parens": [
      1,
      "never"
    ],
    "space-infix-ops": 1,
    "space-unary-ops": [
      1,
      {
        "words": true,
        "nonwords": false
      }
    ],
    "spaced-comment": [
      1,
      "always"
    ]
  }
}
